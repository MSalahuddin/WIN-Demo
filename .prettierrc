{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "bracketSpacing": true,
  "semi": true,
  "useTabs": false,
  "parser": "babel",
  "eslintIntegration": true,
  "jsxBracketSameLine": false,
  "trailing-comma": [
    true,
    {
      "multiline": {
        "objects": "always",
        "arrays": "always",
        "functions": "never",
        "imports": "always",
        "exports": "always"
      },
      "singleline": {
        "objects": "never",
        "arrays": "never",
        "functions": "never",
        "imports": "never",
        "exports": "never"
      }
    }
  ]
}